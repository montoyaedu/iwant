#!/bin/sh

echo "Please enter your app name: "
read APP_NAME

echo "Please enter your package name: "
read PACKAGE_NAME

if [ -z "${APP_NAME}" ]; then
    echo "app name cannot be empty."
    exit 1
fi

if [ -z "${PACKAGE_NAME}" ]; then
    echo "package name cannot be empty."
    exit 1
fi

iwant c# ${APP_NAME} ${PACKAGE_NAME} v4.0 WinExe 1.0.0.0 -SNAPSHOT exe
