#!/bin/sh

echo "Please enter your library name: "
read LIBRARY_NAME

echo "Please enter your package name: "
read PACKAGE_NAME

if [ -z "${LIBRARY_NAME}" ]; then
    echo "library name cannot be empty."
    exit 1
fi

if [ -z "${PACKAGE_NAME}" ]; then
    echo "package name cannot be empty."
    exit 1
fi

iwant c# ${LIBRARY_NAME} ${PACKAGE_NAME} v4.0 Library 1.0.0 -SNAPSHOT dll
